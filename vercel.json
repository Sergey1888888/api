{
  "version": 2,
  "buildCommand": "npm run start",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "vercel/output/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel/output/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}